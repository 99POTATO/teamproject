<main class="community notice_content">
    <section class="section1">
        <div class="sec_wrap">
            <div class="subpage_title_wrap">
                <div class="subpage_title">
                    <div>
                        <h2>N</h2>
                    </div>
                    <div>
                        <h2>E</h2>
                    </div>
                    <div>
                        <h2>W</h2>
                    </div>
                    <div>
                        <h2>S</h2>
                    </div>
                </div>
            </div>
        </div>
        <div class="wave">
            <div class="wave_bg wbg_top"></div>
        </div>
    </section>
    <section class="section2">
        <div class="sec_wrap_b">
            <div class="tab_content_wrap">
                <% let regisDate = row.create_time.getDate(); %> <% let regisMonth = row.create_time.getMonth()+1; %> <% let regisYear =
                row.create_time.getFullYear(); %>
                <div class="notice_cate title"><h3><%= row.title %></h3></div>
                <div class="notice_info">
                    <p>카테고리 : <%= row.category %></p>
                    <p>|</p>
                    <p>작성자 : <%= row.writer %></p>
                    <p>|</p>
                    <p>작성일 : <%= regisYear %>.<%= regisMonth%>.<%= regisDate %></p>
                </div>
                <div class="notice_text">
                    <pre>
<%= row.content %>
                    </pre>
                </div>
            </div>
        </div>
        <div class="notice_btn">
            <input type="button" value="DELETE" class="list_btn" onclick="location.href='/notice_list'" />
            <input type="submit" value="MODIFY" id="submit" />
        </div>

        <div class="wave">
            <div class="wave_bg wbg_bot"></div>
        </div>
    </section>
</main>
<script src="js/event.js"></script>
